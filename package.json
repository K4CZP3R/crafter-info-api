{
  "name": "expressjs-api-boilerplate",
  "version": "1.0.0",
  "description": "My own ExpressJS boilerplate with TS",
  "main": "./src/index.ts",
  "scripts": {
    "postinstall": "npx prisma generate",
    "dev": "DEBUG=expressjs-api-boilerplate* ts-node-dev --exit-child --respawn --pretty --transpile-only ./src/index.ts",
    "dev:win": "set DEBUG=expressjs-api-boilerplate* && ts-node-dev --respawn --pretty --transpile-only ./src/index.ts",
    "try": "ts-node-dev --respawn --pretty --transpile-only ./src/try.ts",
    "prisma:push": "npx prisma db push",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/K4CZP3R/expressjs-api-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^27.3.1",
    "jest-mock-extended": "^2.0.4",
    "prettier": "2.8.7",
    "prisma": "^4.13.0",
    "should": "^13.2.3",
    "supertest": "^6.3.3",
    "ts-jest": "^27.0.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "chai": "^4.3.7",
    "check-password-strength": "^2.0.3",
    "cors": "^2.8.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "get-caller-file": "^2.0.5",
    "helmet": "^4.6.0",
    "jose": "^4.8.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.15",
    "nodemailer": "^6.7.2",
    "pg": "^8.7.1",
    "sha3": "^2.1.4",
    "uuid": "^8.3.2",
    "zod": "^3.21.4"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/K4CZP3R/expressjs-api-boilerplate/issues"
  },
  "homepage": "https://github.com/K4CZP3R/expressjs-api-boilerplate#readme"
}
